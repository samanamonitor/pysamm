[
	{
		"object_type": "command",
		"name": "wmi",
		"type": "sammwr.wmi.WMIQuery",
		"args": {
			"class_name": "$(check.class_name)",
			"endpoint": "http://$(instance.address):5985/wsman",
			"username": "$(resources.cim_username)",
			"password": "$(resources.cim_password)"
		}
	},
	{
		"object_type": "command",
		"name": "snmp_table",
		"type": "sammsnmp.SnmpQuery",
		"args": {
			"hostaddress": "$(instance.address)",
			"community": "$(resources.community)",
			"mib_module_name": "$(check.mib_module_name)",
			"table": "$(check.table)"
		}
	},
	{
		"object_type": "command",
		"name": "snmp_vars",
		"type": "sammsnmp.SnmpQuery",
		"args": {
			"hostaddress": "$(instance.address)",
			"community": "$(instance.community)",
			"mib_module_name": "$(check.mib_module_name)",
			"mib_variable_name_list": "$(check.mib_variable_name_list)"
		}
	},
	{
		"object_type": "command",
		"name": "wrcertificates",
		"type": "sammwr.certificates.WRCertificates",
		"args": {
			"endpoint": "http://$(instance.address):5985/wsman",
			"username": "$(resources.cim_username)",
			"password": "$(resources.cim_password)"
		}
	}
]