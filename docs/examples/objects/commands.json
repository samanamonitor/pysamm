[
	{
		"object_type": "command",
		"name": "ping",
		"type": "samm.pingmodule.PingModule",
		"args": {
			"hostaddress": "$(instance.address)",
			"count": "$(check.count)",
			"timeout": "$(check.timeout)"
		}
	},
	{
		"object_type": "command",
		"name": "cim",
		"type": "sammwr.wmi.WMIQuery",
		"args": {
			"class_name": "$(check.class_name)",
			"endpoint": "http://$(instance.address):5985/wsman",
			"username": "$(resources.cim_username)",
			"password": "$(resources.cim_password)"
		}
	},
	{
		"object_type": "command",
		"name": "snmp_table",
		"type": "sammsnmp.SnmpQuery",
		"args": {
			"hostname": "$(instance.address)",
			"community": "$(instance.community)",
			"version": 2,
			"use_enums": 1,
			"table": "$(check.table)"
		}
	},
	{
		"object_type": "command",
		"name": "snmp_vars",
		"type": "sammsnmp.SnmpQuery",
		"args": {
			"hostname": "$(instance.address)",
			"community": "$(instance.community)",
			"version": 2,
			"use_enums": 1,
			"mib_variable_name_list": "$(check.mib_variable_name_list)"
		}
	},
	{
		"object_type": "command",
		"name": "cim_certificates",
		"type": "sammwr.certificates.WRCertificates",
		"args": {
			"endpoint": "http://$(instance.address):5985/wsman",
			"username": "$(resources.cim_username)",
			"password": "$(resources.cim_password)"
		}
	},
	{
		"object_type": "command",
		"name": "cim_logs",
		"type": "samm.utils.LokiStream",
		"args": {
			"submodule_str": "sammwr.event.CIMEvent",
			"loki_url": "$(resources.loki_url)",
			"endpoint": "http://$(instance.address):5985/wsman",
			"username": "$(resources.cim_username)",
			"password": "$(resources.cim_password)",
			"logfile": "$(check.logfile)",
			"event_type": "$(check.event_type)",
			"time_property": "$(check.time_property)",
			"tag_property": {
				"logfile": "Logfile",
				"instance": "ComputerName"
			},
			"tags": {
				"job": "$(tags.job)"
			}
		}
	},
	{
		"object_type": "command",
		"name": "odata_ctx_director",
		"type": "sammodata.OdataQuery",
		"args": {
			"auth_protocol": "ntlm",
			"username": "$(resources.cim_username)",
			"password": "$(resources.cim_password)",
			"entity": "$(check.entity)",
			"expand": "$(check.expand)",
			"filter": "$(check.filter)",
			"service_url": "http://$(instance.address)/citrix/monitor/odata/v2/Data/"
		}
	}
]
