[
	{
		"object_type": "check",
		"name": "snmp_ns",
		"command": "snmp_vars",
		"mib_variable_name_list": [
			"NS-ROOT-MIB::sysHighAvailabilityMode",
			"NS-ROOT-MIB::resCpuUsage",
			"NS-ROOT-MIB::resMemUsage"
		],
		"tag_properties": [
			"sysHighAvailabilityMode"
		],
		"alias": "snmp_nsstats",
		"check_interval": 60,
		"stale_timeout": 180,
		"metrics": [
			"resCpuUsage",
			"resMemUsage"
		]
	},
	{
		"object_type": "check",
		"name": "snmp_netscaler_certkey",
		"tag_properties": [
			"sslCertKeyName",
			"sslInputFormat"
		],
		"command": "snmp_table",
		"table": "NS-ROOT-MIB::sslCertKeyTable",
		"alias": "snmp_nscertkey",
		"check_interval": 600,
		"stale_timeout": 1800,
		"metrics": [
			"sslDaysToExpire"
		]
	},
	{
		"object_type": "check",
		"name": "snmp_netscaler_appfw",
		"tag_properties": [
			"appfwprofileName",
			"appfwProfileFullName"
		],
		"command": "snmp_table",
		"table": "NS-ROOT-MIB::appfwProfileTable",
		"alias": "snmp_nsappfwstats",
		"check_interval": 60,
		"stale_timeout": 180,
		"metrics": [
			"appfwappFirewallRequestsPerProfile",
			"appfwappFirewallResponsesPerProfile",
			"appfwappFirewallAbortsPerProfile",
			"appfwappFirewallRedirectsPerProfile",
			"appfwappFirewallViolStartURLPerProfile",
			"appfwappFirewallViolDenyURLPerProfile",
			"appfwappFirewallViolRefererHeaderPerProfile",
			"appfwappFirewallViolBufferOverflowPerProfile",
			"appfwappFirewallViolCSRFtagPerProfile",
			"appfwappFirewallViolCookiePerProfile",
			"appfwappFirewallViolXSSPerProfile",
			"appfwappFirewallViolSQLPerProfile",
			"appfwappFirewallViolFieldformatPerProfile",
			"appfwappFirewallViolFieldConsistencyPerProfile",
			"appfwappFirewallViolCreditCardPerProfile",
			"appfwappFirewallViolSafeObjectPerProfile",
			"appfwappFirewallViolWellformednessViolationsPerProfile",
			"appfwappFirewallViolXdosViolationsPerProfile",
			"appfwappFirewallViolMsgValViolationsPerProfile",
			"appfwappFirewallViolWSIViolationsPerProfile",
			"appfwappFirewallViolXmlSqlViolationsPerProfile",
			"appfwappFirewallViolXmlXssViolationsPerProfile",
			"appfwappFirewallViolXmlAttachmentViolationsPerProfile",
			"appfwappFirewallTotalViolPerProfile",
			"appfwappFirewallRet4xxPerProfile",
			"appfwappFirewallRet5xxPerProfile",
			"appfwappFirewallViolXmlSoapFaultViolationsPerProfile",
			"appfwappFirewallReqBytesPerProfile",
			"appfwappFirewallResBytesPerProfile",
			"appfwappFirewallLongAvgRespTimePerProfile",
			"appfwappFirewallShortAvgRespTimePerProfile",
			"appfwappFirewallViolXmlGenericViolationsPerProfile",
			"appfwappFirewallViolSignaturePerProfile",
			"appfwappFirewallViolJsonDosPerProfile",
			"appfwappFirewallViolJsonSqlPerProfile",
			"appfwappFirewallViolJsonXssPerProfile",
			"appfwappFirewallViolFileUploadTypesPerProfile",
			"appfwappFirewallXMLPayloadContentTypeMismatchPerProfile",
			"appfwappFirewallViolCookieHijackPerProfile",
			"appfwappFirewallViolCMDPerProfile",
			"appfwappFirewallPostBodyLimitViolationsPerProfile",
			"appfwappFirewallViolJsonCmdPerProfile",
			"appfwappFirewallViolJsonSqlGramPerProfile",
			"appfwappFirewallViolSQLGramPerProfile"
		]
	},
	{
		"object_type": "check",
		"name": "snmp_netscaler_vserver",
		"tag_properties": [
			"vsvrName",
			"vsvrIpAddress",
			"vsvrType",
			"vsvrState",
			"vsvrTdId",
			"vsvrEntityType"
		],
		"command": "snmp_table",
		"table": "NS-ROOT-MIB::vserverTable",
		"alias": "snmp_nsvserverstats",
		"check_interval": 60,
		"stale_timeout": 180,
		"metrics": [
		   "vsvrCurClntConnections",
		   "vsvrCurSrvrConnections",
		   "vsvrSurgeCount",
		   "vsvrTotalRequests",
		   "vsvrTotalRequestBytes",
		   "vsvrTotalResponses",
		   "vsvrTotalResponseBytes",
		   "vsvrTotalPktsRecvd",
		   "vsvrTotalPktsSent",
		   "vsvrTotalSynsRecvd",
		   "vsvrCurServicesDown",
		   "vsvrCurServicesUnKnown",
		   "vsvrCurServicesOutOfSvc",
		   "vsvrCurServicesTransToOutOfSvc",
		   "vsvrCurServicesUp",
		   "vsvrTotMiss",
		   "vsvrTotHits",
		   "vsvrTotSpillOvers",
		   "vsvrTotalClients",
		   "vsvrCurSslVpnUsers",
		   "vsvrTotalServicesBound",
		   "vsvrHealth",
		   "vsvrTicksSinceLastStateChange",
		   "vsvrTotalServers",
		   "vsvrInvalidRequestResponse",
		   "vsvrInvalidRequestResponseDropped",
		   "vsvrSoThreshold",
		   "vsvrEstablishedConn",
		   "vsvrCurTotalVpnUsers",
		   "vsvrCurMptcpSessions",
		   "vsvrCursubflowConn",
		   "vsvrCurPersistenceSessions",
		   "vsvrCurBackupPersistenceSessions",
		   "vsvrtotalConnReassemblyQueueFlush",
		   "vsvrtotalConnReassemblyQueue75",
		   "vsvrtotalSvrBusyErr",
		   "vsvrTotalH2Responses",
		   "vsvrTotalH2Requests",
		   "vsvrCpuUsagePm"
		]
	},
	{
		"object_type": "check",
		"name": "snmp_netscaler_ipstats",
		"command": "snmp_table",
		"table": "NS-ROOT-MIB::nsIpStatsGroup",
		"alias": "snmp_nsipstats",
		"check_interval": 60,
		"stale_timeout": 180,
		"metrics": [
			"ipTotRxPkts",
			"ipTotRxBytes",
			"ipTotRxMbits",
			"ipTotTxPkts",
			"ipTotTxBytes",
			"ipTotTxMbits",
			"ipTotFragments",
			"ipTotBadlens",
			"ipTotBadMacAddrs",
			"ipTotMaxClients",
			"ipTotUnknownSvcs",
			"ipTotLandattacks",
			"ipTotBadChecksums",
			"ipTotReassemblyAttempt",
			"ipTotSuccReassembly",
			"ipTotUnsuccReassembly",
			"ipTotTooBig",
			"ipTotZeroFragmentLen",
			"ipTotDupFragments",
			"ipTotOutOfOrderFrag",
			"ipTotUnknownDstRcvd",
			"ipTotBadTransport",
			"ipTotVIPDown",
			"ipTotFixHeaderFail",
			"ipTotAddrLookup",
			"ipTotAddrLookupFail",
			"ipTotUDPfragmentsFwd",
			"ipTotTCPfragmentsFwd",
			"ipTotFragPktsGen",
			"ipTotInvalidHeaderSz",
			"ipTotInvalidPacketSize",
			"ipTotTruncatedPackets",
			"ipTotZeroNextHop",
			"ipTotTtlExpired",
			"nonIpTotTruncatedPackets"
		]
	}
]
